generator db {
  provider = "go run github.com/steebchen/prisma-client-go"
}

datasource db {
  provider = "mysql"
  url      = "mysql://mikatan:password@db:3306/mikatan_knitting"
}

model users {
  userId    String   @id
  name      String
  email     String
  password  String
  createdAt DateTime @default(now())
  pudatedAt DateTime
}
